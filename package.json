{
  "name": "www-week-5",
  "version": "1.0.0",
  "description": "Simple 5x5 Tic Tac Toe game using vanilla Javascript, and express js and mongodb for persistence.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "npm run prod",
    "server": "npx babel-node ./src/server/index.js",
    "server:prod": "node ./dist/server/index.js",
    "dev": "NODE_ENV=development npm-run-all server",
    "clean": "rimraf dist",
    "build": "npx babel ./src --out-dir dist",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "watch": "nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NikodemiusTKT/www-week-5.git"
  },
  "keywords": [
    "tic-tac-toe",
    "vanilla-js",
    "expressjs",
    "mongodb",
    "cs-assignment"
  ],
  "author": "Teemu Tanninen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NikodemiusTKT/www-week-5/issues"
  },
  "homepage": "https://github.com/NikodemiusTKT/www-week-5#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.19",
    "lodash.clonedeep": "^4.5.0",
    "morgan": "^1.9.1",
    "npm-run-all": "^4.1.5",
    "pug": "^2.0.4",
    "rimraf": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.0",
    "@babel/node": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "babel-plugin-lodash": "^3.3.4",
    "nodemon": "^1.19.4",
    "parcel-bundler": "^1.12.4"
  }
}
